<div class="containert"></div>
<h3 class="text-center mt-5 mb-3">プロフィール編集</h3>
<div class="container">
  <div class="row">
    <div class="col-sm-2"></div>
    <div class="col-sm-8 mt-4">
      <%= form_with model: @enduser, method: :patch do |f| %>
      <label for="inputName" >ユーザー名</label><br>
      <%= f.text_field :name, autofocus: true, id:"inputName", placeholder: "名前" %><br>
      <label for="inputImage">プルフィール画像</label>
      <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>
      <label for="inputEmail">メールアドレス</label><br>
      <%= f.email_field :email, autofocus: true, id:"inputEmail", placeholder: "メールアドレス" %>
      <%= f.submit "変更を保存", class: "btn btn-success" %>
      <% end %>
    </div>
  </div>
</div>